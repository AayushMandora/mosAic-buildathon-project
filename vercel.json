{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|json|txt|woff|woff2|ttf|map))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}